<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="visao.ResolverProblemas">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Selecionar Problema" />
            <ComboBox fx:id="comboBoxProblemas" prefWidth="150.0" />
            <HBox prefHeight="12.0" prefWidth="560.0" spacing="20.0">
               <children>
                  <Button fx:id="btnVoltar" mnemonicParsing="false" onAction="#onClickVoltar" text="Voltar" />
                  <Button fx:id="btnResolver" mnemonicParsing="false" onAction="#onClickResolver" text="Resolvido" />
               </children>
               <padding>
                  <Insets bottom="20.0" right="20.0" top="20.0" />
               </padding>
            </HBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
